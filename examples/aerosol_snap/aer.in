[BISPECTRUM]
numTypes = 3
twojmax = 12
rcutfac = 5.0
rfac0 = 0.99363
rmin0 = 0.0
wj1 = 1.0
wj2 = 2.1 
wj3 = 3.0
radelem1 = 0.30e+00 #HP
radelem2 = 0.56e+00 #HP
radelem3 = 0.60e+00 #HP
type1 = H
type2 = C
type3 = O

[CALCULATOR]
wselfallflag = 1
chemflag = 0
bnormflag = 1
bzeroflag = 0   
quadraticflag = 1
stress = 0
force = 0

[GROUPS]
group_sections = name training_size testing_size eweight 
group_types = str float float float 

[SCRAPER]
scraper = xyz
property_array =
	positions = length metal metal
	lattice = length metal metal
	forces = force metal metal
	energy = energy metal metal

[SOLVER]
solver = SVD	
dump_a = 1
dump_x = 1
dump_b = 1

[PATH]
dataPath = ./data
groupFile = ./grouplist.in

[OUTFILE]
configs = aerosol_configs.pkl.gz
metrics = aerosol_metrics.csv
potential = aerosol_pot

[REFERENCE]
units = metal
atom_style = atomic
pair_style = zero 10.0
pair_coeff1 = * *
